{
  "name": "georgringer/news-seo",
  "keywords": [
    "TYPO3",
    "extension",
    "news",
    "seo"
  ],
  "authors": [
    {
      "name": "Georg Ringer",
      "role": "Developer",
      "homepage": "https://ringer.it"
    }
  ],
  "type": "typo3-cms-extension",
  "description": "Indivdual indexing/robot information for each news article record",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": ">= 8.1 < 8.5",
    "georgringer/news": "^10 || ^11 || ^12",
    "typo3/cms-seo": "^12 || ^13"
  },
  "autoload": {
    "psr-4": {
      "GeorgRinger\\NewsSeo\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GeorgRinger\\NewsSeo\\Tests\\": "Tests"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "news_seo"
    }
  }
}
